ömischen Gesellschaft zu erklären und untergrub deren festgefügte Ordnung."/><type4:Token xmi:id="2" sofa="1" begin="0" end="3"/><type4:Token xmi:id="3" sofa="1" begin="4" end="14"/><type4:Token xmi:id="4" sofa="1" begin="15" end="25"/><type4:Token xmi:id="5" sofa="1" begin="26" end="34"/><type4:Token xmi:id="6" sofa="1" begin="35" end="40"/><type4:Token xmi:id="7" sofa="1" begin="41" end="48"/><type4:Token xmi:id="8" sofa="1" begin="49" end="51"/><type4:Token xmi:id="9" sofa="1" begin="52" end="61"/><type4:Token xmi:id="10" sofa="1" begin="62" end="65"/><type4:Token xmi:id="11" sofa="1" begin="66" end="80"/><type4:Token xmi:id="12" sofa="1" begin="81" end="86"/><type4:Token xmi:id="13" sofa="1" begin="87" end="89"/><type4:Token xmi:id="14" sofa="1" begin="90" end="96"/><type4:Token xmi:id="15" sofa="1" begin="97" end="103"/><type4:Token xmi:id="16" sofa="1" begin="104" end="112"/><type4:Token xmi:id="17" sofa="1" begin="113" end="119"/><type4:Token xmi:id="18" sofa="1" begin="120" end="123"/><type4:Token xmi:id="19" sofa="1" begin="124" end="131"/><type4:Token xmi:id="20" sofa="1" begin="132" end="135"/><type4:Token xmi:id="21" sofa="1" begin="136" end="142"/><type4:Token xmi:id="22" sofa="1" begin="142" end="143"/><type4:Token xmi:id="23" sofa="1" begin="144" end="147"/><type4:Token xmi:id="24" sofa="1" begin="148" end="150"/><type4:Token xmi:id="25" sofa="1" begin="151" end="166"/><type4:Token xmi:id="26" sofa="1" begin="167" end="170"/><type4:Token xmi:id="27" sofa="1" begin="171" end="179"/><type4:Token xmi:id="28" sofa="1" begin="180" end="183"/><type4:Token xmi:id="29" sofa="1" begin="184" end="200"/><type4:Token xmi:id="30" sofa="1" begin="201" end="208"/><type4:Token xmi:id="31" sofa="1" begin="208" end="209"/><type4:Token xmi:id="32" sofa="1" begin="210" end="212"/><type4:Token xmi:id="33" sofa="1" begin="213" end="219"/><type4:Token xmi:id="34" sofa="1" begin="220" end="226"/><type4:Token xmi:id="35" sofa="1" begin="227" end="232"/><type4:Token xmi:id="36