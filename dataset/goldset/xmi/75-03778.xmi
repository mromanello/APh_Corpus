erungen über die « doppelte Erschaffung des Menschen » feststellen."/><type4:Sentence xmi:id="2" sofa="1" begin="0" end="367"/><type4:Token xmi:id="3" sofa="1" begin="0" end="6"/><type4:Token xmi:id="4" sofa="1" begin="7" end="19"/><type4:Token xmi:id="5" sofa="1" begin="20" end="23"/><type4:Token xmi:id="6" sofa="1" begin="24" end="35"/><type4:Token xmi:id="7" sofa="1" begin="36" end="38"/><type4:Token xmi:id="8" sofa="1" begin="39" end="43"/><type4:Token xmi:id="9" sofa="1" begin="44" end="48"/><type4:Token xmi:id="10" sofa="1" begin="49" end="50"/><type4:Token xmi:id="11" sofa="1" begin="51" end="53"/><type4:Token xmi:id="12" sofa="1" begin="54" end="58"/><type4:Token xmi:id="13" sofa="1" begin="59" end="72"/><type4:Token xmi:id="14" sofa="1" begin="73" end="74"/><type4:Token xmi:id="15" sofa="1" begin="75" end="82"/><type4:Token xmi:id="16" sofa="1" begin="83" end="88"/><type4:Token xmi:id="17" sofa="1" begin="89" end="98"/><type4:Token xmi:id="18" sofa="1" begin="99" end="102"/><type4:Token xmi:id="19" sofa="1" begin="103" end="114"/><type4:Token xmi:id="20" sofa="1" begin="115" end="121"/><type4:Token xmi:id="21" sofa="1" begin="122" end="133"/><type4:Token xmi:id="22" sofa="1" begin="134" end="145"/><type4:Token xmi:id="23" sofa="1" begin="146" end="150"/><type4:Token xmi:id="24" sofa="1" begin="151" end="154"/><type4:Token xmi:id="25" sofa="1" begin="155" end="166"/><type4:Token xmi:id="26" sofa="1" begin="167" end="170"/><type4:Token xmi:id="27" sofa="1" begin="171" end="179"/><type4:Token xmi:id="28" sofa="1" begin="180" end="182"/><type4:Token xmi:id="29" sofa="1" begin="183" end="190"/><type4:Token xmi:id="30" sofa="1" begin="191" end="192"/><type4:Token xmi:id="31" sofa="1" begin="193" end="196"/><type4:Token xmi:id="32" sofa="1" begin="197" end="198"/><type4:Token xmi:id="33" sofa="1" begin="199" end="200"/><type4:Token xmi:id="34" sofa="1" begin="201" end="206"/><type4:Token xmi:id="35" sofa="1" begin="207" end="219"/><type4:Token xmi:id="36" sofa="1"