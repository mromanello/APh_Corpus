ple."/><type4:Sentence xmi:id="2" sofa="1" begin="0" end="150"/><type4:Sentence xmi:id="3" sofa="1" begin="151" end="323"/><type4:Token xmi:id="4" sofa="1" begin="0" end="2"/><type4:Token xmi:id="5" sofa="1" begin="3" end="11"/><type4:Token xmi:id="6" sofa="1" begin="12" end="13"/><type4:Token xmi:id="7" sofa="1" begin="14" end="19"/><type4:Token xmi:id="8" sofa="1" begin="20" end="23"/><type4:Token xmi:id="9" sofa="1" begin="24" end="31"/><type4:Token xmi:id="10" sofa="1" begin="32" end="33"/><type4:Token xmi:id="11" sofa="1" begin="34" end="36"/><type4:Token xmi:id="12" sofa="1" begin="37" end="43"/><type4:Token xmi:id="13" sofa="1" begin="44" end="47"/><type4:Token xmi:id="14" sofa="1" begin="48" end="50"/><type4:Token xmi:id="15" sofa="1" begin="51" end="59"/><type4:Token xmi:id="16" sofa="1" begin="60" end="67"/><type4:Token xmi:id="17" sofa="1" begin="68" end="70"/><type4:Token xmi:id="18" sofa="1" begin="71" end="76"/><type4:Token xmi:id="19" sofa="1" begin="77" end="83"/><type4:Token xmi:id="20" sofa="1" begin="84" end="88"/><type4:Token xmi:id="21" sofa="1" begin="89" end="91"/><type4:Token xmi:id="22" sofa="1" begin="92" end="98"/><type4:Token xmi:id="23" sofa="1" begin="99" end="108"/><type4:Token xmi:id="24" sofa="1" begin="109" end="118"/><type4:Token xmi:id="25" sofa="1" begin="119" end="123"/><type4:Token xmi:id="26" sofa="1" begin="124" end="126"/><type4:Token xmi:id="27" sofa="1" begin="127" end="134"/><type4:Token xmi:id="28" sofa="1" begin="135" end="140"/><type4:Token xmi:id="29" sofa="1" begin="141" end="149"/><type4:Token xmi:id="30" sofa="1" begin="149" end="150"/><type4:Token xmi:id="31" sofa="1" begin="151" end="155"/><type4:Token xmi:id="32" sofa="1" begin="156" end="158"/><type4:Token xmi:id="33" sofa="1" begin="159" end="169"/><type4:Token xmi:id="34" sofa="1" begin="170" end="172"/><type4:Token xmi:id="35" sofa="1" begin="173" end="186"/><type4:Token xmi:id="36" sofa="1" begin="187" end="200"/><type4:Token xmi:id="37" sofa="1" begin="201