erk gesetzt zu haben."/><type4:Sentence xmi:id="2" sofa="1" begin="0" end="310"/><type4:Sentence xmi:id="3" sofa="1" begin="311" end="379"/><type4:Sentence xmi:id="4" sofa="1" begin="380" end="478"/><type4:Token xmi:id="5" sofa="1" begin="0" end="3"/><type4:Token xmi:id="6" sofa="1" begin="4" end="14"/><type4:Token xmi:id="7" sofa="1" begin="15" end="18"/><type4:Token xmi:id="8" sofa="1" begin="19" end="24"/><type4:Token xmi:id="9" sofa="1" begin="25" end="26"/><type4:Token xmi:id="10" sofa="1" begin="27" end="31"/><type4:Token xmi:id="11" sofa="1" begin="32" end="37"/><type4:Token xmi:id="12" sofa="1" begin="38" end="39"/><type4:Token xmi:id="13" sofa="1" begin="40" end="44"/><type4:Token xmi:id="14" sofa="1" begin="45" end="54"/><type4:Token xmi:id="15" sofa="1" begin="55" end="62"/><type4:Token xmi:id="16" sofa="1" begin="63" end="66"/><type4:Token xmi:id="17" sofa="1" begin="67" end="70"/><type4:Token xmi:id="18" sofa="1" begin="71" end="79"/><type4:Token xmi:id="19" sofa="1" begin="80" end="90"/><type4:Token xmi:id="20" sofa="1" begin="91" end="98"/><type4:Token xmi:id="21" sofa="1" begin="99" end="102"/><type4:Token xmi:id="22" sofa="1" begin="103" end="113"/><type4:Token xmi:id="23" sofa="1" begin="114" end="117"/><type4:Token xmi:id="24" sofa="1" begin="118" end="134"/><type4:Token xmi:id="25" sofa="1" begin="135" end="141"/><type4:Token xmi:id="26" sofa="1" begin="142" end="145"/><type4:Token xmi:id="27" sofa="1" begin="146" end="155"/><type4:Token xmi:id="28" sofa="1" begin="155" end="156"/><type4:Token xmi:id="29" sofa="1" begin="157" end="160"/><type4:Token xmi:id="30" sofa="1" begin="161" end="164"/><type4:Token xmi:id="31" sofa="1" begin="165" end="177"/><type4:Token xmi:id="32" sofa="1" begin="178" end="191"/><type4:Token xmi:id="33" sofa="1" begin="192" end="197"/><type4:Token xmi:id="34" sofa="1" begin="198" end="199"/><type4:Token xmi:id="35" sofa="1" begin="200" end="203"/><type4:Token xmi:id="36" sofa="1" begin="204" end="214"/><type4:Token xmi:i