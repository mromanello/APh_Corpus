irect Athenian imperialism expressed in the Standards Decree (IG I3 1453)."/><type4:Sentence xmi:id="2" sofa="1" begin="0" end="121"/><type4:Sentence xmi:id="3" sofa="1" begin="122" end="343"/><type4:Token xmi:id="4" sofa="1" begin="0" end="1"/><type4:Token xmi:id="5" sofa="1" begin="2" end="8"/><type4:Token xmi:id="6" sofa="1" begin="9" end="19"/><type4:Token xmi:id="7" sofa="1" begin="20" end="24"/><type4:Token xmi:id="8" sofa="1" begin="25" end="31"/><type4:Token xmi:id="9" sofa="1" begin="32" end="36"/><type4:Token xmi:id="10" sofa="1" begin="37" end="40"/><type4:Token xmi:id="11" sofa="1" begin="41" end="48"/><type4:Token xmi:id="12" sofa="1" begin="49" end="54"/><type4:Token xmi:id="13" sofa="1" begin="55" end="58"/><type4:Token xmi:id="14" sofa="1" begin="59" end="65"/><type4:Token xmi:id="15" sofa="1" begin="66" end="68"/><type4:Token xmi:id="16" sofa="1" begin="69" end="77"/><type4:Token xmi:id="17" sofa="1" begin="78" end="80"/><type4:Token xmi:id="18" sofa="1" begin="81" end="84"/><type4:Token xmi:id="19" sofa="1" begin="85" end="97"/><type4:Token xmi:id="20" sofa="1" begin="98" end="101"/><type4:Token xmi:id="21" sofa="1" begin="102" end="112"/><type4:Token xmi:id="22" sofa="1" begin="113" end="120"/><type4:Token xmi:id="23" sofa="1" begin="120" end="121"/><type4:Token xmi:id="24" sofa="1" begin="122" end="125"/><type4:Token xmi:id="25" sofa="1" begin="126" end="131"/><type4:Token xmi:id="26" sofa="1" begin="132" end="135"/><type4:Token xmi:id="27" sofa="1" begin="136" end="147"/><type4:Token xmi:id="28" sofa="1" begin="148" end="150"/><type4:Token xmi:id="29" sofa="1" begin="151" end="157"/><type4:Token xmi:id="30" sofa="1" begin="158" end="165"/><type4:Token xmi:id="31" sofa="1" begin="166" end="168"/><type4:Token xmi:id="32" sofa="1" begin="169" end="172"/><type4:Token xmi:id="33" sofa="1" begin="173" end="179"/><type4:Token xmi:id="34" sofa="1" begin="180" end="181"/><type4:Token xmi:id="35" sofa="1" begin="181" end="189"/><type4:Token xmi:id="36" so